{
  "manifest_version": 2,

  "name": "Motivator!",
  "description": "This extension is developed to keep motivating you by showing the minutes left in the day!!",
  "version": "1",

  

  "author": "Pavan Kalyan Komarina",

  "browser_action": {
   "default_title": "Motivator!",
   "default_popup":"popup.html"
  },

  "chrome_url_overrides" : {
    "newtab": "customNewTab.html"
  },

  "permissions": [
    "activeTab",
    "storage",
    "http://api.forismatic.com/"
  ],

  "icons": { 
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png" 
  }
}